{
  "expo": {
    "name": "ImaginPaws",
    "slug": "imaginpaws",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/appicon2.png",
    "scheme": "imaginpaws",
    "deepLinking": true,
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.imaginpaws.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": ["imaginpaws"]
          }
        ]
      }
    },
    "android": {
      "package": "com.imaginpaws.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/appicon2.png",
        "backgroundColor": "#ffffff"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "data": {
            "scheme": "imaginpaws",
            "host": "*"
          },
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-dev-client"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0d146bd6-645b-4257-b083-e8b4ff22e10f"
      }
    }
  }
}
